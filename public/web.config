<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="AstroProxy">
                    <match url="^(.*)$" />
                    <action type="Rewrite" url="http://127.0.0.1:4321/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
